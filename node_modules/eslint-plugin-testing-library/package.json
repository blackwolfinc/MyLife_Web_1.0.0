  "readmeFilename": "README.md",
  "release": {
    "pkgRoot": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testing-library/eslint-plugin-testing-library.git"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write README.md {lib,docs,tests}/**/*.{js,md}",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "postbuild": "cpy README.md ./dist && cpy package.json ./dist && cpy LICENSE ./dist",
    "semantic-release": "semantic-release",
    "test": "is-ci test:ci test:local",
    "test:ci": "jest --coverage",
    "test:local": "jest",
    "test:update": "npm run test:local -- --u",
    "test:watch": "npm run test:local -- --watch"
  },
  "version": "3.10.1",
  "warnings": [
    {
      "code": "ENOTSUP",
      "required": {
        "node": "^10.12.0 || >=12.0.0",
        "npm": ">=6"
      },
      "pkgid": "eslint-plugin-testing-library@3.10.1"
    }
  ]
}
